<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.failed_access</string>

  <key>ProgramArguments</key>
  <array>
    <string>/etc/failed_access.sh</string>
  </array>

  <key>Nice</key>
  <integer>1</integer>

  <key>StartInterval</key>
  <integer>300</integer>

  <key>RunAtLoad</key>
  <true></true>

  <key>StandardErrorPath</key>
  <string>/tmp/failed_access.err</string>

  <key>StandardOutPath</key>
  <string>/tmp/failed_access.out</string>
</dict>
</plist>